<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                     xmlns:vm="clr-namespace:CryptoCurrency.ViewModel"
                    xmlns:page="clr-namespace:CryptoCurrency.Pages">

    <DataTemplate DataType="{x:Type vm:CoinsVM}">
        <page:Coin/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SearchVM}">
        <page:Search/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:ConvertVM}">
        <page:Convert/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:FavoriteVM}">
        <page:Favorite/>
    </DataTemplate>

</ResourceDictionary>